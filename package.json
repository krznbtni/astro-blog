{
  "name": "@krznbtni/astro-blog",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && npm run lint:eslint && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "format": "npm run format:code",
